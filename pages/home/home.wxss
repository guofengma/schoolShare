/* pages/home/home.wxss */
zan-tab{
  position: fixed;
  z-index: 100;
}

.fixBar{
  position: fixed;
  bottom: 50rpx;
  right: 30rpx;
  z-index: 200;
}
.fixBar-item{
  width: 100rpx;
  height: 100rpx;
  border: 1px solid #ccc;
  border-radius: 50%;
  color: white;
   background-color: #f59763; 
  text-align: center;
  margin-top:20rpx;
}
.fixBar-item .iconfont{
  display: inline-block;
    font-size: 50rpx;
    line-height: 100rpx;
}
.weui-search-bar {
  position: fixed;
  top: 0;
  width: 100%;
  display: none;
}

.weui-navbar {
  position: fixed;
  background-color: #eee;
}

.weui-tab__panel {
  position: relative;
}




